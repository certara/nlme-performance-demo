$PK 
  CWT = WT/70 
  TVVMAX= THETA(1) 
  VMAX=TVVMAX*EXP(ETA(1)) 
  TVKM = THETA(2) 
  KM = TVKM 
  TVV2=THETA(3) 
  V2=TVV2 
  TVKA=THETA(4) 
  KA=TVKA 
  SC = V2 
$ERROR 
  REP = IREP 
  IPRED = F 
  Y=F*EXP(EPS(2)) + EPS(1) 
$DES 
  CONC = A(2)/SC 
  DADT(1) = -KA*A(1) 
  DADT(2) =  KA*A(1)-VMAX*CONC/(KM+CONC) 
 $TABLE ID TIME DV MDV NOPRINT NOHEADER NOAPPEND FILE=OBS_DAT 
 $TABLE ID REP TIME DV IPRED PRED MDV NOPRINT NOHEADER NOAPPEND FILE=SIM_DAT 
