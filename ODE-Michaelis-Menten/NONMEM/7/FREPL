$PK 
  CWT = WT/70
  TVVMAX= THETA(1)
  VMAX=TVVMAX*EXP(ETA(1))
  TVKM = THETA(2)
  KM = TVKM
  TVV2=THETA(3)
  V2=TVV2
  TVKA=THETA(4)
  KA=TVKA
  K23=THETA(5)
  K32=THETA(6)
  SC = V2
$ERROR 
  Y=F*EXP(EPS(2)) + EPS(1)
$DES 
  CONC = A(2)/SC
  DADT(1) = -KA*A(1)
  DADT(2) =  KA*A(1)-VMAX*CONC/(KM+CONC) -K23*A(2)+K32*A(3)
  DADT(3) = K23*A(2)-K32*A(3)
